
.preview {
	height: 400rpx;
	margin: 10rpx 25rpx;
	border-radius: 30rpx;
	overflow: hidden;
	margin-bottom: 60rpx;
	box-shadow: 10rpx 10rpx 20rpx rgba(0, 0, 0, 0.2), 10rpx 10rpx 20rpx rgba(255, 255, 255, 1);
}
.preview image {
	width: 100%;
	height: 100%;
}
.title {
	margin-top:70rpx;
	font-size: 35rpx;
}
.title .colLine {
	margin: auto 25rpx;
	color: blue;
	font-weight: bold;
}
.content{
	width: 95%;
	margin:  30rpx auto;
	height: 175rpx;		
	display:-webkit-box;		
	display:-webkit-flex;		
	display:flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.nav{
	width: 30%;
	height: 100%;
	border-radius: 15rpx;
	box-shadow: 10rpx 10rpx 20rpx rgba(0, 0, 0, 0.2), -10rpx -10rpx 20rpx rgba(255, 255, 255, 1);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.nav image{
	width: 35%;
	height: 40%;
	margin-bottom: 20rpx;
}
.notice {
	margin-top: 50rpx;
}
.noticeContent {
	background-color: white;
	width: 94%;
	height: 60rpx;
	margin: 15rpx auto;
	border-radius: 15rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.noticeContent image {
	width: 50rpx;
	height: 50rpx;
	margin: auto 10rpx;
}

